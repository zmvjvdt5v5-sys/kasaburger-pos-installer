  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "electron-dev": "cross-env ELECTRON_DEV=true concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron ./public/electron.js\"",
    "electron-build": "yarn build && electron-builder",
    "electron-pack": "electron-builder --dir"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.kasaburger.kbys",
    "productName": "KBYS",
    "copyright": "Copyright Â© 2026 Kasa Burger",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "public/electron.js",
      "public/preload.js",
      "public/logo192.png"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "public/logo192.png"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "public/logo192.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "public/logo192.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "public/logo192.png",
      "uninstallerIcon": "public/logo192.png"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
